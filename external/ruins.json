    {
        "id": 9,
        "description": "Trap in the building.",
        "data": [
            {
                "roll": {
                    "from": 11,
                    "to": 16
                },
                "info": "Falling Door - Attack with five Base Dice, Weapon Damage 1 (blunt force). - First PC",
                "additionalRoll": null
            },
            {
                "roll": {
                    "from": 21,
                    "to": 23
                },
                "info": "Swinging hammer - Attack with six Base Dice, Weapon Damage 2 (blunt force). - First two PCs",
                "additionalRoll": null
            },
            {
                "roll": {
                    "from": 24,
                    "to": 26
                },
                "info": "Fungal spores - Hallucinogenic poison with Potency 6. - All",
                "additionalRoll": null
            },
            {
                "roll": {
                    "from": 31,
                    "to": 33
                },
                "info": "Visions of the elven civil war - Fear attack with six Base Dice. Anyone who suffers damage sees a vision of the Winter King on his throne. - All",
                "additionalRoll": null
            },
            {
                "roll": {
                    "from": 34,
